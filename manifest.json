{
    "name": "Chrome Toolbox",
    "description": "General set of tools for a power user",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "options/hello.html",
        "default_icon": "icons/hello_extensions.png"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+F",
                "mac": "MacCtrl+Shift+F"
            },
            "description": "Opens hello.html"
        }
    }
}